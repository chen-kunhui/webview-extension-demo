{
	"name": "extension-demo",
	"displayName": "vscode webview extension demo",
	"description": "vscode webview 类型插件模板",
	"version": "0.0.1",
	"publisher": "ckh",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsWelcome": [
			{
				"view": "extension-demo.sidebar",
				"contents": "加载中 ..."
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "extension-demo-demo",
					"title": "extension-demo",
					"icon": "logo.svg"
				}
			],
			"panel": [
				{
					"id": "extension-demo-panel-demo",
					"title": "extension-demo",
					"icon": "monkey.svg"
				}
			]
		},
		"views": {
			"extension-demo-demo": [
				{
					"type": "webview",
					"id": "extension-demo.sidebar",
					"name": "示例"
				}
			],
			"extension-demo-panel-demo": [
				{
					"type": "webview",
					"id": "extension-demo.panel",
					"name": "panel webview 示例"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "extension-demo.open-in-edit",
					"when": "view == extension-demo.sidebar",
					"group": "navigation"
				}
			]
		},
		"commands": [
			{
			  "command": "extension-demo.open-in-edit",
			  "title": "在编辑区打开",
			  "icon": "$(chrome-restore)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts"
	},
	"devDependencies": {
		"@types/node": "14.x",
		"@types/vscode": "^1.66.0",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"eslint": "^8.11.0",
		"simple-webview": "file:lib",
		"ts-loader": "^9.2.8",
		"typescript": "^4.5.5",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2"
	}
}